generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Entry {
  id           Int        @id @default(autoincrement())
  username     String     @default("")
  form0Answer  Json?
  form1Answer  Json?      
  form2Answer  Json?
  form3Answer  Json?
  form4Answer  Json?
  form5Answer  Json?
  form6Answer  Json?
  form7Answer  Json?
  form8Answer  Json?
  form9Answer  Json?
  form10Answer Json?
  createdAt    DateTime   @default(now())
  updatedAt    DateTime   @updatedAt @default(now())
}

model Form {
  id           Int        @id @default(autoincrement())
  formId       String     @default("")
  text         String     @default("")
  choices      Json?      
  status       String     @default("")
  maxchoices   Int        @default(1)
  frozen       Boolean    @default(false)
  createdAt    DateTime   @default(now())
  updatedAt    DateTime   @updatedAt @default(now())
}